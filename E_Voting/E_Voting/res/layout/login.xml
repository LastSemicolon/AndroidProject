<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <ImageView
        android:id="@+id/imagesview12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img7" />

    <ImageButton
        android:id="@+id/imagesimple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="61dp"
        android:src="@drawable/proceedtovote" />

    <ImageButton
        android:id="@+id/imagecamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imagesimple"
        android:layout_below="@+id/imagesimple"
        android:layout_marginTop="68dp"
        android:src="@drawable/camp_camp" />
 
</RelativeLayout>
